{
	"name": "llp-sveltekit",
	"version": "0.0.1",
	"packageManager": "pnpm@7.32.4",
	"scripts": {
		"dev": "vite dev",
		"build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"netlify": "netlify dev --live",
		"functions": "netlify functions:serve --port 9000",
		"stripe": "stripe listen --forward-to localhost:5173/api/stripe-webhook",
		"prepare": "svelte-kit sync",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "prisma generate"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.90",
		"@sveltejs/adapter-netlify": "^1.0.1",
		"@sveltejs/kit": "^1.10.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.3",
		"@types/bootstrap": "^5.2.6",
		"@types/ccapture.js": "^1.1.1",
		"@types/luxon": "^3.2.0",
		"@types/mapbox-gl": "^2.7.10",
		"@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
		"@types/mapbox__tilebelt": "^1.0.0",
		"@types/node": "^18.14.1",
		"@types/sortablejs": "^1.15.0",
		"eslint-config-custom": "workspace:*",
		"prettier": "^2.8.0",
		"prettier-plugin-prisma": "^4.10.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.14.0",
		"sass": "^1.53.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^4.0.0"
	},
	"dependencies": {
		"@aws-sdk/client-mediaconvert": "^3.287.0",
		"@aws-sdk/client-s3": "^3.281.0",
		"@aws-sdk/client-ses": "^3.295.0",
		"@aws-sdk/s3-presigned-post": "^3.281.0",
		"@lucia-auth/adapter-prisma": "^0.6.1",
		"@lucia-auth/sveltekit": "^0.6.11",
		"@lucia-auth/tokens": "^0.2.0",
		"@mapbox/tilebelt": "^1.0.2",
		"@mapbox/togeojson": "^0.16.0",
		"@netlify/functions": "^1.4.0",
		"@paralleldrive/cuid2": "^2.2.0",
		"@planetscale/database": "^1.7.0",
		"@prisma/client": "^4.14.0",
		"@turf/turf": "^6.5.0",
		"@types/geojson": "^7946.0.10",
		"@uppy/core": "^3.2.0",
		"@uppy/file-input": "^3.0.2",
		"@uppy/status-bar": "^3.1.2",
		"@uppy/transloadit": "^3.1.4",
		"bootstrap": "^5.2.3",
		"ccapture.js": "^1.1.0",
		"cuid": "^3.0.0",
		"form-data": "^4.0.0",
		"land-model": "workspace:*",
		"lucia-auth": "^0.10.0",
		"luxon": "^3.3.0",
		"map": "workspace:*",
		"mapbox-gl": "^2.12.0",
		"nanoevents": "^7.0.1",
		"node-fetch": "^3.3.1",
		"puppeteer-core": "^19.7.5",
		"sortablejs": "^1.15.0",
		"stripe": "^11.15.0",
		"sveltekit-flash-message": "^0.11.3",
		"sveltekit-superforms": "^0.6.1",
		"sveltestrap": "^5.10.0",
		"vanilla-lazyload": "^17.8.3",
		"vite-plugin-commonjs": "^0.6.1",
		"zod": "^3.20.6"
	},
	"type": "module"
}

{
	"name": "search-site",
	"version": "0.0.0",
	"scripts": {
		"dev": "vite dev --port 3001",
		"build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
		"package": "vite package",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --ignore-path=../../.prettierignore . && eslint \"src\"",
		"format": "prettier --write --ignore-path=../../.prettierignore .",
		"prepare": "svelte-kit sync",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"apexcharts": "^3.40.0",
		"bootstrap": "^5.2.3",
		"d3": "^7.8.4",
		"land-model": "workspace:*",
		"map": "workspace:*",
		"sveltestrap": "^5.10.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "latest",
		"@sveltejs/adapter-netlify": "^2.0.7",
		"@sveltejs/kit": "latest",
		"autoprefixer": "^10.4.14",
		"eslint-config-custom": "workspace:*",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^4.0.0",
		"vite-plugin-commonjs": "^0.6.1",
		"vite-plugin-externals": "^0.6.2"
	},
	"type": "module"
}
